using Gtk 4.0;
using Adw 1;

template $ReaderPage: Adw.NavigationPage {
    title: "Viewer";

    Adw.ToolbarView {
        [top]
        Adw.HeaderBar {}

        Adw.Carousel carousel {
            focusable: true;
        }

        [bottom]
        Adw.CarouselIndicatorDots {
            carousel: carousel;
        }
    }
}
