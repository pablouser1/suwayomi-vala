        <key name="url" type="s">
            <default>'http://localhost:4567'</default>
            <summary>Suwayomi Base URL</summary>
            <description>URL of Suwayomi server.</description>
        </key>
        <key name="username" type="s">
            <default>''</default>
            <summary>Suwayomi Username</summary>
            <description>Username for accessing the GraphQL API.</description>
        </key>
        <key name="password" type="s">
            <default>''</default>
            <summary>Suwayomi Password</summary>
            <description>Password for accessing the GraphQL API.</description>
        </key>
    </schema>
</schemalist>
